<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_constraintBottom_toTopOf="parent"
    tools:context=".Profile">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="422dp"
            android:layout_height="73dp"
            app:srcCompat="@drawable/header" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/backbutton"
                android:layout_width="55dp"
                android:layout_height="25dp"
                android:layout_marginTop="3dp"
                android:layout_marginRight="220dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/backicon" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="-220dp"
                android:layout_marginTop="1dp"
                android:layout_weight="1"
                android:text="@string/back"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="normal|bold" />

        </LinearLayout>

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="138dp"
            android:layout_height="136dp"
            android:layout_marginLeft="130dp"

            android:layout_marginTop="5dp"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/profile" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="@string/uname"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="24sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="@string/ename"
            android:textAlignment="center"
            android:textColor="@color/material_dynamic_neutral20"
            android:textSize="18sp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/button5"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="75dp"
            android:layout_marginTop="10dp"
            android:backgroundTint="@color/Btn_color"
            android:text="@string/edit"
            android:textAlignment="center"
            android:textSize="18sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="58dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal"
            android:paddingRight="100dp">

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="41dp"
                android:layout_height="33dp"
                android:layout_marginTop="10dp"
                android:layout_weight="1"
                android:paddingLeft="20dp"
                app:srcCompat="@drawable/settingi" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginRight="40dp"
                android:layout_weight="1"
                android:text="@string/settings"
                android:textAlignment="textStart"
                android:textColor="@color/black"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="58dp"
            android:layout_marginTop="4dp"
            android:orientation="horizontal"
            android:paddingRight="100dp">

            <ImageView
                android:id="@+id/imageaddevent"
                android:layout_width="41dp"
                android:layout_height="37dp"
                android:layout_marginTop="10dp"
                android:layout_weight="1"
                android:paddingLeft="20dp"
                app:srcCompat="@drawable/addeventi" />

            <TextView
                android:id="@+id/textevent"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginRight="30dp"
                android:layout_weight="1"
                android:text="@string/addevent"
                android:textAlignment="textStart"
                android:textColor="@color/black"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="58dp"
            android:layout_marginTop="4dp"
            android:orientation="horizontal"
            android:paddingRight="100dp">

            <ImageView
                android:id="@+id/imagesupport"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_marginTop="10dp"
                android:layout_weight="1"
                android:paddingLeft="20dp"
                app:srcCompat="@drawable/supporti" />

            <TextView
                android:id="@+id/textsupport"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginRight="30dp"
                android:layout_weight="1"
                android:text="@string/support"
                android:textAlignment="textStart"
                android:textColor="@color/black"
                android:textStyle="bold" />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="58dp"
            android:layout_marginTop="4dp"
            android:orientation="horizontal"
            android:paddingRight="100dp">

            <ImageView
                android:id="@+id/imageshare"
                android:layout_width="19dp"
                android:layout_height="37dp"
                android:layout_marginTop="10dp"
                android:layout_weight="1"
                android:paddingLeft="20dp"
                app:srcCompat="@drawable/sharei" />

            <TextView
                android:id="@+id/textshare"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginRight="30dp"
                android:layout_weight="1"
                android:text="@string/share"
                android:textAlignment="textStart"
                android:textColor="@color/black"
                android:textStyle="bold" />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="58dp"
            android:layout_marginTop="4dp"
            android:orientation="horizontal"
            android:paddingRight="100dp">

            <ImageView
                android:id="@+id/imageabtus"
                android:layout_width="41dp"
                android:layout_height="34dp"
                android:layout_marginTop="10dp"
                android:layout_weight="1"
                android:paddingLeft="20dp"
                app:srcCompat="@drawable/abouti" />

            <TextView
                android:id="@+id/textaboutus"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginRight="30dp"
                android:layout_weight="1"
                android:text="@string/aboutus"
                android:textAlignment="textStart"
                android:textColor="@color/black"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="75dp"
            android:layout_marginTop="40dp"
            android:background="@color/Purple"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/homeimage4"
                android:layout_width="94dp"
                android:layout_height="41dp"
                android:layout_marginTop="15dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/homei" />

            <ImageView
                android:id="@+id/listimage4"
                android:layout_width="91dp"
                android:layout_height="53dp"
                android:layout_marginTop="12dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/listi" />

            <ImageView
                android:id="@+id/favimage4"
                android:layout_width="89dp"
                android:layout_height="59dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/favoritei" />

            <ImageView
                android:id="@+id/profileimage4"
                android:layout_width="92dp"
                android:layout_height="45dp"
                android:layout_marginTop="14dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/profilei" />
        </LinearLayout>


    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>